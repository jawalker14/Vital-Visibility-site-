{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()" },
        { "key": "Content-Security-Policy", "value": "default-src 'self'; img-src 'self' data: https:; script-src 'self' https://challenges.cloudflare.com; style-src 'self'; font-src 'self' data:; connect-src 'self'; frame-src 'self' https://challenges.cloudflare.com" }
      ]
    },
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|webp|avif)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=60" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [ { "type": "host", "value": "vitalvisibility.com" } ],
      "destination": "https://vitalvisibility.co.za/:path*",
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "has": [ { "type": "host", "value": "www.vitalvisibility.com" } ],
      "destination": "https://vitalvisibility.co.za/:path*",
      "statusCode": 301
    }
  ],
  "trailingSlash": false
}
